year = int(input('Please enter year = '))
month = int(input('Please enter month = '))
day = int(input('Please enter day = '))
hour = int(input('Please enter hour = '))
min = int(input('Please enter minute = '))
sec = int(input('Please enter sec = '))
x  = (year*365*86400) + (month*30*86400) + (day*86400) + (hour*3600) + (min*60) + (sec)
print(x)
